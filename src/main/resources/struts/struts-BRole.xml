<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 2.1//EN"
        "http://struts.apache.org/dtds/struts-2.1.dtd">
<struts>
	<package name="bRole" extends="base" namespace="/bRole">
		<action name="bRole_*" method="{1}" class="com.item.web.action.BRoleAction">
			<result name="list">/WEB-INF/content/bRole/bRole_list.jsp</result>
			<result name="delete" type="redirect">${ctx}/bRole/bRole_list.shtml</result>
			<result name="save" type="redirect">${ctx}/bRole/bRole_list.shtml</result>
			<result name="handle">/WEB-INF/content/bRole/bRole_handle.jsp</result>
			<result name="view">/WEB-INF/content/bRole/bRole_view.jsp</result>
		</action>
	</package>
</struts>